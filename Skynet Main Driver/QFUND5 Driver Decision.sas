												-CONFIG 'C:\PROGRAM FILES\SASHOME\SASFOUNDATION\9.4\SASV9.CFG'"
								TASKNAME=QF5_TRAN_DRIVE
								STATUS=QF5_TRAN_DRIVE;							
							%END;

						/*DEALSUMMARY*/
						%IF &DEAL_QF5_PAYDAY_STATUS. = NOT_COMPLETE
							AND %EVAL(&DEAL_QF5_PAYDAY_RUN_DATE. = .)
							AND &DEAL_QF5_INSTALL_STATUS. = NOT_COMPLETE
							AND %EVAL(&DEAL_QF5_INSTALL_RUN_DATE. = .)
							AND &DEAL_QF5_TITLE_STATUS. = NOT_COMPLETE
