								TASKNAME=QF5_DEAL_DRIVE
								STATUS=QF5_DEAL_DRIVE;
							%END;

						/*DAILY*/
						%IF &DAIL_QF5_PAYDAY_STATUS. = NOT_COMPLETE
							AND %EVAL(&DAIL_QF5_PAYDAY_RUN_DATE. = .)
							AND &DAIL_QF5_INSTALL_STATUS. = NOT_COMPLETE
							AND %EVAL(&DAIL_QF5_INSTALL_RUN_DATE. = .)
							AND &DAIL_QF5_TITLE_STATUS. = NOT_COMPLETE
