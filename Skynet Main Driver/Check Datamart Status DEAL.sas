/*-----------------------------------------------------------CUSTOMER DATAMART---------------------------------------------------*/

/*EADV*/
PROC SQL NOPRINT;
	SELECT EADV_STATUS
		  ,DATEPART(EADV_RUN_DATE)
		  ,DATEPART(EADV_COMPLETION_DATE)
				INTO :DEAL_EADV_STATUS
				   , :DEAL_EADV_RUN_DATE
				   , :DEAL_EADV_COMPLETION_DATE
	FROM BIOR.DATAMART_STATUS
	WHERE SOURCE = 'BIOR.O_DEAL_SUMMARY_ALL'
;
QUIT;

